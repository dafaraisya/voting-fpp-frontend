AAA;AACA;AACA,OArBA;AAsBA,KAxBA;AAyBA,IAAA,QAzBA,oBAyBA,GAzBA,EAyBA;AACA,aAAA,WAAA,GAAA;AACA;AA3BA,GAbA;AA0CA,EAAA,OA1CA,qBA0CA;AAAA;;AACA,IAAA,KAAA,CACA,GADA,CACA,sDAAA,KAAA,MAAA,CAAA,MAAA,CAAA,EADA,EAEA,IAFA,CAEA,UAAA,GAAA;AAAA,aAAA,MAAA,CAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AAAA,KAFA,EAGA,KAHA,CAGA,UAAA,GAAA;AAAA,aAAA,OAAA,CAAA,GAAA,CAAA,GAAA,CAAA;AAAA,KAHA;AAIA;AA/CA,CAAA","sourcesContent":["<template>\n    <b-container>\n        <b-row>\n            <b-col lg=\"6\">\n                <div class=\"bg-white p-3 rounded-sm shadow\">\n                    <h3>{{ detail.name }}</h3>\n                    <img class=\"img-profile img-profile-circle\" v-bind:src=\"getImage(detail.image)\"/><br/><br/>\n                    <b>Nomor urut </b><br/>{{ detail.number }}<br/>\n                    <b>Deskripsi</b>\n                    <div v-html=\"detail.description.short\"/>\n                    <b>Visi</b>\n                    <div v-html=\"detail.description.vision\"/>\n                    <b>Misi</b>\n                    <div v-html=\"detail.description.mission\"/><br/>\n                    <a v-bind:href=\"'http://pemira.fppundip.com:3000/'+this.$route.params.id+'.pdf'\">\n                        <b-button class=\"ml-2\" href=\"\" variant=\"primary\">\n                            <i class=\"far fa-file-alt text-white\"></i>\n                            CV\n                        </b-button>\n                    </a>\n                    <router-link :to=\"'edit/'+detail._id\">\n                        <b-button class=\"ml-2\" href=\"\" variant=\"primary\">\n                            <i class=\"far fa-edit text-white\"></i>\n                            Ubah\n                        </b-button>\n                    </router-link>\n                    <b-button @click=\"del();\" class=\"ml-2\" href=\"\" variant=\"primary\">\n                        <i class=\"far fa-trash-alt text-white\"></i>\n                        Hapus\n                    </b-button>\n                </div>\n            </b-col>\n        </b-row>\n    </b-container>\n</template>\n<script>\nimport axios from 'axios'\nimport Swal from 'sweetalert2'\n\nexport default {\n    name: 'CandidateDetail',\n    data() {\n        return {\n            detail: {\n                description: {\n                    short: '',\n                    vision: '',\n                    mission: ''\n                }\n            }\n        }\n    },\n    methods:{\n        del(){\n            Swal.fire({\n                title: 'Apakah anda yakin menghapus kandidat ini?',\n                showDenyButton: true,\n                confirmButtonText: `Ya`,\n                denyButtonText: `Tidak`,\n            }).then((result) => {\n                if (result.isConfirmed) {            \n                    axios\n                        .delete(\"http://pemira.fppundip.com:3000/api/v1/candidate/\"+this.$route.params.id)\n                        .then(() => {\n                            Swal.fire({\n                                icon: 'success',\n                                title: 'Kandidat berhasil dihapus',\n                                showConfirmButton: true\n                            }).then(()=>{\n                                this.$router.push({name: 'ListCandidate'});\n                            })\n                        })\n                        .catch( err => console.log(err));\n                    Swal.fire('Saved!', '', 'success')\n                } \n            })\n        },\n        getImage(url) {\n            return '../../'+url;\n        }\n    },\n    created() {\n        axios\n            .get(\"http://pemira.fppundip.com:3000/api/v1/candidate/\"+this.$route.params.id)\n            .then(res => (this.detail = res.data.data))\n            .catch(err => console.log(err));\n    }\n}\n</script>\n<style>\n.img-profile-circle {\n    height: 200px;\n    width: 200px;\n    border-radius: 100px;\n}\n</style>"],"sourceRoot":"src/views/admin/candidate"}]}